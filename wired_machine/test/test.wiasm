ram_size:  640000
stack_size:40000

ENTRY:
    scall 0
    
    load  ra rb
    loadi ra 128
    loada ra 0xFFFFF

    save ra rb
    savei ra 128
    savea  0xFFFFF ra
    saveai 0xFFFFF 0128

    add   ra rb rc
    addi  ra rb 128
    min   ra rb rc
    mini  ra rb 128
    mult  ra rb rc
    multi ra rb 128
    div   ra rb rc
    divi  ra rb 128

    and   ra rb rc
    andi  ra rb 128
    or    ra rb rc
    ori   ra rb 128
    xor   ra rb rc
    xori  ra rb 128
    not   ra rb 
    noti  ra 128

    eq    ra rb
    eqi   ra 128
    neq   ra rb
    neqi  ra 128

    gt    ra rb
    gti   ra 128
    gte   ra rb
    gtei  ra 128    

    lt    ra rb
    lti   ra 128
    lte   ra rb
    ltei  ra 128

FLAG:
    jre   ra 
    jrei  0xffff
    jrei  FLAG 

    jeq   ra 
    jeqi  0xffff
    jeqi  FLAG 

    jne   ra 
    jnei  0xffff
    jnei  FLAG 

    jmp   ra 
    jmpi  0xffff
    jmpi  FLAG 

    jmp_th   ra 
    jmp_thi  0xffff
    jmp_thi  FLAG 